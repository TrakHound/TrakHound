<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>TrakHound API</Name>
    <Description>A project to create a API Package for use in the TrakHound Framework</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType></ProjectSubType>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>ExampleApi</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.png</Icon>
    <LanguageTag>csharp</LanguageTag>
    <PlatformTag>windows</PlatformTag>
    <ProjectTypeTag>trakhound</ProjectTypeTag>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="TrakHound.Template.Api.csproj" File="TrakHound.Template.Api.csproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="Controller.cs">Controller.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="Program.cs">Program.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="trakhound.package.json">trakhound.package.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="trakhound.package.publish.json">trakhound.package.publish.json</ProjectItem>
    </Project>
    <CustomParameters>
      <CustomParameter Name="$apiroute$" Value="example/api"/>
    </CustomParameters>
  </TemplateContent>
</VSTemplate>